generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_DB_URL")
  shadowDatabaseUrl = env("POSTGRES_DB_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  email     String
  password  String
  createdAt DateTime @default(now())
}
